{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true
  },
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "alias-skip.mappings": {
    "@": "/src"
  }
}
